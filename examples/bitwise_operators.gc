module main


func main() Void {
    printint(13 | 3) // 15
    putchar('\n')

    printint(13 & 3) // 1
    putchar('\n')

    printint(13 ^ 3) // 14
    putchar('\n')

    printint(~13) // -14
    putchar('\n')
}


func printint(n Int) Void {
    if n < 0 {
        putchar('-')
        n = -n
    }
    if n / 10 > 0 {
        printint(n / 10)
    }
    putchar(n % 10 + '0')
}
