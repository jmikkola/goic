module main


func main() Void {
    printint(10 << 1)
    putchar('\n')

    printint(10 << 2)
    putchar('\n')

    printint(10 >> 1)
    putchar('\n')

    printint(10 >> 2)
    putchar('\n')
}


func printint(n Int) Void {
    if n / 10 > 0 {
        printint(n / 10)
    }
    putchar(n % 10 + '0')
}

